{
    "name": "TestXsis",
    "version": "1.0.0",
    "description": "API Testing Xsis",
    "main": "index.js",
    "scripts": {
        "test": "jest --watchAll --detectOpenHandles",
        "test-sequelize": "jest --verbose src/sequelize",
        "watch": "npx tsc --build --watch",
        "migration": "cd src/db/prisma/Xsis && npx prisma migrate dev",
        "add-migration-wh": "cd src/db/prisma/Xsis && npx prisma migrate dev --create-only",
        "apply-migration-wh": "cd src/db/prisma/Xsis  && npx prisma migrate deploy",
        "migrate": "npx prisma generate && npm run apply-migration-wh",
        "start": "node ./build/index.js",
        "dev": "nodemon index.ts"
    },
    "keywords": [],
    "author": "M Haikal Anatama",
    "license": "ISC",
    "jest": {
        "maxConcurrency": 5
    },
    "dependencies": {
        "@prisma/client": "^4.11.0",
        "@types/errorhandler": "^1.5.0",
        "@types/json-diff": "^0.7.0",
        "@types/moment-timezone": "^0.5.30",
        "body-parser": "^1.19.2",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.3",
        "express-validator": "^6.14.0",
        "helmet": "^5.0.2",
        "jest": "^27.5.1",
        "json-diff": "^0.7.4",
        "lodash": "^4.17.21",
        "moment": "^2.29.3",
        "moment-timezone": "^0.5.34",
        "mysql2": "^2.3.3",
        "sequelize": "^6.20.1",
        "tsconfig-paths": "^4.0.0",
        "typed-slack": "^0.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.17.10",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@types/body-parser": "^1.19.2",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/cron": "^2.0.0",
        "@types/ejs": "^3.1.1",
        "@types/express": "^4.17.13",
        "@types/express-fileupload": "^1.2.2",
        "@types/handlebars-helpers": "^0.5.3",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.23",
        "@types/sequelize": "^4.28.13",
        "nodemon": "^2.0.15",
        "prisma": "^4.11.0",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src",
            "index.ts"
        ],
        "exec": "ts-node",
        "ext": "ts, js"
    }
}
